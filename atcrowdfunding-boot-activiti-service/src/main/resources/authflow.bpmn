    <sequenceFlow id="flow10" name="上一步" sourceRef="exclusivegateway3" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${flag==false}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="mailtask1" name="发送验证码" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[admin@atguigu.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[邮箱验证码]]></activiti:string>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string><![CDATA[UTF-8]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[众筹网验证码为：${authcode}]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow11" name="下一步" sourceRef="exclusivegateway3" targetRef="mailtask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${flag==true}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask5" name="邮箱验证" activiti:assignee="${loginacct}"></userTask>
    <sequenceFlow id="flow12" sourceRef="mailtask1" targetRef="usertask5"></sequenceFlow>
    <userTask id="usertask6" name="实名认证审核" activiti:candidateGroups="backcheck"></userTask>
    <sequenceFlow id="flow13" sourceRef="usertask5" targetRef="usertask6"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow14" sourceRef="usertask6" targetRef="exclusivegateway4"></sequenceFlow>
    <endEvent id="endevent1" name="End">
      <extensionElements>
        <activiti:executionListener event="end" class="com.atguigu.atcrowdfunding.activiti.listener.RefuseListener"></activiti:executionListener>
      </extensionElements>
    </endEvent>
    <sequenceFlow id="flow15" name="审批拒绝" sourceRef="exclusivegateway4" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${flag==false}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End">
      <extensionElements>
        <activiti:executionListener event="end" class="com.atguigu.atcrowdfunding.activiti.listener.PassListener"></activiti:executionListener>
      </extensionElements>
    </endEvent>
    <sequenceFlow id="flow16" name="审批通过" sourceRef="exclusivegateway4" targetRef="endevent2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${flag==true}]]></conditionExpression>
    </sequenceFlow>
