    <sequenceFlow id="flow7" name="下一步" sourceRef="exclusivegateway2" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${flag==true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" name="上一步" sourceRef="exclusivegateway2" targetRef="usertask2">
